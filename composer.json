{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "belphemur/laravel-gettext": "^6.0",
        "chumper/zipper": "^1.0",
        "faustbrian/laravel-commentable": "^3.0",
        "fideloper/proxy": "~3.3",
        "garf/laravel-conf": "3.*",
        "ghanem/rating": "^1.4",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "6.2",
        "guzzlehttp/psr7": "1.3",
        "kim/activity": "^1.1",
        "kodeine/laravel-meta": "dev-master",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "laravelista/ekko": "^1.4",
        "spatie/laravel-medialibrary": "^6.0.0",
        "spatie/laravel-tags": "^2.0",
        "venturecraft/revisionable": "^1.28",
        "webmozart/assert": "^1.2",
        "yab/laravel-scout-mysql-driver": "^2.0",
        "zizaco/entrust": "^1.8"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "doctrine/dbal": "~2.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
